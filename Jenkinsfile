pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo this is new build'
            }
        }
        stage('Test') {
            steps {
                sh 'echo this is new test'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo this is new deploy'
            }
        }
    }
}